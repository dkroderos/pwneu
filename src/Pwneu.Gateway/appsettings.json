{
  "ReverseProxy": {
    "Routes": {
      "play-route": {
        "ClusterId": "play-cluster",
        "Match": {
          "Path": "play/{**catch-all}"
        },
        "Transforms": [
          {
            "PathPattern": "{**catch-all}"
          }
        ]
      },
      "identity-route": {
        "ClusterId": "identity-cluster",
        "Match": {
          "Path": "identity/{**catch-all}"
        },
        "Transforms": [
          {
            "PathPattern": "{**catch-all}"
          }
        ]
      },
      "chat-route": {
        "ClusterId": "chat-cluster",
        "Match": {
          "Path": "chat/{**catch-all}"
        },
        "Transforms": [
          {
            "PathPattern": "{**catch-all}"
          }
        ]
      }
    },
    "Clusters": {
      "play-cluster": {
        "Destinations": {
          "destination1": {
            "Address": "https://pwneu.play:8081"
          }
        },
        "HttpClient": {
          "DangerousAcceptAnyServerCertificate": "true"
        }
      },
      "identity-cluster": {
        "Destinations": {
          "destination1": {
            "Address": "https://pwneu.identity:8081"
          }
        },
        "HttpClient": {
          "DangerousAcceptAnyServerCertificate": "true"
        }
      },
      "chat-cluster": {
        "Destinations": {
          "destination1": {
            "Address": "https://pwneu.chat:8081"
          }
        },
        "HttpClient": {
          "DangerousAcceptAnyServerCertificate": "true"
        }
      }
    }
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*"
}
